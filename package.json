{
  "name": "fpl_next",
  "version": "0.0.1",
  "description": "FPL data webapp front end",
  "main": "index.js",
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}",
      "layout/**/*.{js,jsx}",
      "lib/**/*.{js,jsx}",
      "pages/**/*.{js,jsx}",
      "utils/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:coverage": "npm run test --coverage",
    "lint": "./node_modules/.bin/eslint ."
  },
  "author": "Michael Cockcroft",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-http": "^1.5.17",
    "dotenv": "^6.1.0",
    "graphql": "^15.3.0",
    "next": "^11.1.3",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-ga": "^2.6.0",
    "react-spinners": "^0.9.0",
    "styled-jsx": "^3.2.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "babel-jest": "^24.7.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "prettier": "2.1.1"
  }
}
